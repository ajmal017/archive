{
	"name": "YouTube BS-kipper",
	"version": "1.0",
	"description": "Skip that video bs and get to the content",
	"permissions": [ "activeTab", "declarativeContent", "storage"],
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"page_action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "assets/icons/get_started16.png",
			"32": "assets/icons/get_started32.png",
			"48": "assets/icons/get_started48.png",
			"128": "assets/icons/get_started128.png"
		}
	},
	"icons": {
		"16": "assets/icons/get_started16.png",
		"32": "assets/icons/get_started32.png",
		"48": "assets/icons/get_started48.png",
		"128": "assets/icons/get_started128.png"
	},
	"manifest_version": 2
}
